[push]
	default = simple
[alias]
    lg = log --graph --abbrev-commit --decorate --date=relative \
        --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)%n          %C(white)%s%C(reset)' \
        --all
    pp = !git pull --rebase --autostash; and git push;

[pull]
	rebase = true
